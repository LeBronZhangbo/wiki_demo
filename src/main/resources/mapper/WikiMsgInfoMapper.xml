<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.zhangbo.wiki_back.mapper.WikiMsgInfoMapper">


    <update id="createTableByID" parameterType="String">
        CREATE TABLE t_wiki_msg_info_${id}
        SELECT *
        FROM t_wiki_msg_info
        where 1 = 2
    </update>
</mapper>
